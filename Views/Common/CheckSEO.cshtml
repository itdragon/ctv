@model List<string>

<div>
    <div class="modal-header">
        <a href="#" class="close" data-dismiss="modal">&times;</a>
        <h4>
            <span id="popTitle">Thông tin SEO</span>
        </h4>
        <div class="clear">
        </div>
    </div>
    <div class="maincontent" style="text-align:left;overflow:auto;height:360px">
        @Html.Raw(Model[1])
    </div>
    <div class="clear"></div>
    <div class="maincontent">
        <div id="popMsg" style="color: red"></div>
        <input type="submit" id="btnOK" @if (Model[0] == "0")
                                        {<text>disabled="disabled"</text>}  class="btn btn-warning"
                value="Đồng ý" />
        <input type="button" id="btnCancel" onclick="CancelUpdateNews()" class="btn btn-warning"
            value="Hủy bỏ" />
    </div>
</div>
